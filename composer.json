{
    "name": "setjs/etuke",
    "description": "A video system about edu.",
    "keywords": ["video", "edu", "laravel"],
    "homepage": "http://www.etuke.cn",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "barryvdh/laravel-cors": "^0.11.2",
        "chumper/zipper": "^1.0",
        "emojione/emojione": "^3.1",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "hyancat/aliyun-direct-mail": "dev-master",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^3.2",
        "laravel/tinker": "~1.0",
        "league/html-to-markdown": "^4.8",
        "maatwebsite/excel": "^3.0",
        "mews/purifier": "^2.0",
        "overtrue/pinyin": "~3.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "socialiteproviders/qq": "^3.0",
        "spatie/laravel-backup": "5.10.1",
        "symfony/process": "^4.2",
        "yansongda/laravel-pay": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^4.0",
        "mockery/mockery": "~1.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "./app/helper.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "./vendor/bin/phpunit",
        "cs-fixer": "php php-cs-fixer.phar fix",
        "cf": "php-cs-fixer fix",
        "clear-app-cache": [
            "php artisan route:clear",
            "php artisan config:clear",
            "php artisan view:clear"
        ],
        "build-app-cache": [
            "php artisan route:cache",
            "php artisan config:cache"
        ],
        "restart-supervisor": [
            "supervisorctl reread",
            "supervisorctl update",
            "supervisorctl start etuke:*"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    }
}
